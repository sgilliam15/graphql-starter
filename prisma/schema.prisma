datasource postgresql {
  provider = "postgresql"
  url      = "postgresql://docker_admin:password@localhost:5432/postgresql_db?schema=public"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id      Int     @default(autoincrement()) @id
  name    String?
  email   String  @unique
  isAdmin Boolean @default(false)
}
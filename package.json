{
  "name": "node-graphql-starter",
  "version": "1.0.0",
  "description": "A GraphQL server using Typescript Node, Prisma 2+ and Apollo Server",
  "author": "Steven R Gilliam Jr <steve@steveg.dev>",
  "main": "build/index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "tsc && npm run lint",
    "serve": "node build/index.js",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
  },
  "dependencies": {
    "async": "^3.1.0",
    "tslib": "^1.11.1"
  },
  "keywords": [
    "node",
    "typescript",
    "prisma",
    "apollo-server",
    "graphQL",
    "docker"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}